<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.collections.FXCollections?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controler.ControllerTelaAdminTorneio" alignment="CENTER" spacing="10">
    <GridPane alignment="CENTER" vgap="5" hgap="5">
        <Text text="Torneio de:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
        <ComboBox GridPane.rowIndex="0" GridPane.columnIndex="1" prefWidth="100">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Futebol"/>
                    <String fx:value="Volei"/>
                    <String fx:value="Basquete"/>
                    <String fx:value="Outros.."/>
                </FXCollections>
            </items>
        </ComboBox>
        <Button text="Seguir" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
    </GridPane>
</VBox>
